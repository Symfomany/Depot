{
    "name": "jms/serializer-bundle",
    "type": "symfony-bundle",
    "description": "Allows you to easily serialize, and deserialize object graphs of any complexity",
    "keywords": ["serialization", "deserialization", "json", "jaxb", "xml"],
    "homepage": "http://jmsyst.com/bundles/JMSSerializerBundle",
    "license": "Apache2",
    "authors": [
        {
            "name": "Johannes M. Schmitt",
            "email": "schmittjoh@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "jms/metadata": ">=1.1.0,<1.3-dev"
    },
    "require-dev": {
        "symfony/framework-bundle": ">=2.0,<2.2-dev",
        "symfony/yaml": ">=2.0,<2.2-dev",
        "symfony/form": ">=2.0,<2.2-dev",
        "symfony/validator": ">=2.0,<2.2-dev",
        "twig/twig": ">=1.8,<2.0-dev",
        "doctrine/orm": ">=2.1,<2.4-dev"
    },
    "autoload": {
        "psr-0": { "JMS\\SerializerBundle": "" }
    },
    "target-dir": "JMS/SerializerBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.9.x-dev"
        }
    }
}
